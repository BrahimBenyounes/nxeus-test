pipeline {
    agent any
    stages {
 
        stage('Test and compile') {
            steps {
                echo "*****************Cleaning...."
                sh "mvn clean"
                echo "*****************Completed clean"
                
                sh "mvn compile"
                echo "*****************Finish Compiling"
                echo '*****************Run unit tests'
                sh "mvn package"
                echo '*****************Package finished'
            }
        }
        stage('Deploy') {
            steps {
                echo "*****************Start deploy"
                sh "mvn -Dmaven.test.skip=true -Dmaven.main.skip=true deploy"
            }
        }
    }
}

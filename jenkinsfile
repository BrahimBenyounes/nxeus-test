pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
           
             git 'https://github.com/BrahimBenyounes/nxeus-test.git'
                sh "ls -ll"
            }
        }
       
        stage('Deploy') {
            steps {
               echo "*****************Start deploy"
               sh "mvn deploy"
            }
        }
    }
}
